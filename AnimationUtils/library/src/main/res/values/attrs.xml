<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="ShoppingCartPathBezierView">
        <attr name="scv_src" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RatingBar" tools:ignore="ResourceName">
        <attr name="rb_starImageSize" format="dimension"/>
        <attr name="starImageWidth" format="dimension"/>
        <attr name="starImageHeight" format="dimension"/>
        <attr name="starImagePadding" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <attr name="starNum" format="integer"/>
        <attr name="starEmpty" format="reference"/>
        <attr name="starFill" format="reference"/>
        <attr name="starHalf" format="reference"/>
        <attr name="clickable" format="boolean"/>
        <attr name="halfstart" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="LineAnimation" tools:ignore="ResourceName">
        <attr name="lineHeight" format="dimension|reference"/>
        <attr name="lineWidth" format="dimension|reference"/>
        <attr name="lineColor" format="color"/>
        <attr name="mCurrentOrientation" format="integer"/>
        <attr name="isLong2Short" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="UserLikeAnimationView">
        <attr name="ulv_src1" format="reference"/>
        <attr name="ulv_src2" format="reference"/>
        <attr name="ulv_src3" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WaitingDots">
        <attr name="dotsColor" format="color" />
        <attr name="period" format="integer"/>
        <attr name="jumpHeight" format="integer"/>
        <attr name="autoplay" format="boolean"/>
    </declare-styleable>
</resources>